{
    "name": "arbkomekvw/evangtermine",
    "type": "typo3-cms-extension",
    "description": "Integrates the output of the \"Evangelische Termine\" web application into TYPO3 web sites",
    "homepage": "https://github.com/abkekvw/evangtermine",
    "keywords": [
        "TYPO3 CMS",
        "ekvw",
        "termine",
        "front-end",
        "evangtermine"
    ],
    "license": ["GPL-2.0+"],
    "authors": [
        {
            "name": "Christoph Roth",
            "email": "christoph.roth@lka.ekvw.de",
            "role": "Developer",
            "homepage": "http://www.ekvw.de/kommunikation/"
        }
    ],
    "require": {
        "php": "5.6 - 7.1",
        "ext-SimpleXML": "*",
        "ext-mbstring": "*",
        "typo3/cms-core": "^7.6.0 || ^8.7.0"
    },
    "autoload": {
        "psr-4": {
            "ArbkomEKvW\\Evangtermine\\": "Classes/"
        }
    },
    "replace": {
        "evangtermine": "self.version",
        "typo3-ter/evangtermine": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "evangtermine"
        }
    }
}
